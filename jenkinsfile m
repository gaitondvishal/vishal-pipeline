pipeline {
     agent any
     
     environment {
  BRANCH = "main"
  GIT_REPO = "https://github.com/gaitondvishal/vishal-pipeline.git"
}
     stages {
      stage('BUILD') {
       steps {
            sh '''
                #!/bin/bash
                pwd
                ls
                echo "this is a build stage"
          '''
          }
      }
      stage('DEPLOY') {
         steps {
               echo "This is DEPLOY stage"
          }
      }
      stage ('TESTING1') {
         steps {
          sh 'echo "This is a TESTING1 stage"'
      }
   }
   stage ('TESTING2') {
       steps {
           sh '''
              echo "This is a TESTING2 stage"
            '''
            }
         }
       }
    }
